Page{
  background-color: #fff;
}

.info{
  font-weight: bold;
  font-size: 60rpx;
  margin-top: 80rpx;
}

.cu-form-group {
  background-color: var(--white);
  padding: 1rpx 0rpx;
  display: flex;
  min-height: 100rpx;
  justify-content: space-between;
}
.cu-form-group .title {
  font-size: 36rpx;
}
.cu-form-group input{
  font-size: 36rpx;
}

.cu-form-group textarea {
  font-size: 36rpx;
  height: 10em;
}
.apply_btn {
  width: 80%;
  padding: 0 40rpx;
  font-size: 36rpx;
  height: 80rpx;
  color: var(--white);
  font-weight: bold;
}
  